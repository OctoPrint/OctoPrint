<div>
	<div class='file_search' style='display: none'>
		<form class="form-search" data-bind="submit: performSearch">
			<input type="text" class="input-block search-query" data-bind="value: searchQuery, valueUpdate: 'input'" placeholder="{{ _('Search...') }}">
		</form>
	</div>
	<div class="bocusini_gcode_files" data-bind="slimScrolledForeach: listHelper.paginatedItems">
		<div class="entry" data-bind="attr: { id: $root.getEntryId($data) }, template: { name: $root.templateForBocusini($data), data: $data }"></div>

		<script type="text/html" id="bocusini_files_template_machinecode">
			<div class="flip-container" onclick="this.classList.toggle('flip');" ontouchstart="this.classList.toggle('hover');">
				<div class="flipper">
					<div class="front">
						<div class="gcode_thumb" data-bind="style:{'background-image': $root.bocusiniThumbnail($data)}"> 
							<div class="title" data-bind="css: $root.getSuccessClass($data), style: { 'font-weight': $root.listHelper.isSelected($data) ? 'bold' : 'normal' }, text: $root.bocusiniFilename($data)"></div>
						</div>
					</div>
					<div class="back">
						<div class="additionalInfo" data-bind="html: $root.getAdditionalData($data)"></div>
						<div class="btn-group action-buttons">
							<div class="btn btn-large" data-bind="click: function() {if ($root.enableRemove($data)) { $root.removeFile($data); $root.bocusiniRemoveThumbnail($data); } else { return; } }, css: {disabled: !$root.enableRemove($data)}" title="{{ _('Remove') }}"><i class="icon-trash"></i></div>
							<!-- <div class="btn btn-large" data-bind="click: function() { if ($root.enableRemove($data)) { $root.removeFile($data); } else { return; } }, css: {disabled: !$root.enableRemove($data)}" title="{{ _('Remove') }}"><i class="icon-trash"></i></div> -->														
							<!-- <a class="btn btn-large dl_btn" data-bind="attr: {href: $root.downloadLink($data), css: {disabled: !$root.downloadLink($data)}}" title="{{ _('Download') }}"><i class="icon-download-alt"></i></a>-->
						 	<!-- <div class="btn btn-large"  data-bind="click: function() { if ($root.enableSelect($data)) { $root.loadFile($data, true); } else { return; } }, css: {disabled: !$root.enableSelect($data)}" title="{{ _('Load and Print') }}"><i class="icon-print"></i></div> -->
						        <div class="btn btn-large"  data-bind="click: function() { if ($root.enableSelectFile($data)) { $root.loadFile($data, true); } else { return; } }, css: {}" title="{{ _('Load and Print') }}"><i class="icon-print"></i></div>
						</div>
					</div>
				</div>
			</div>
		</script>

		<script type="text/html" id="bocusini_files_template_model">
			<div class="title muted" data-bind="text: name"></div>
			<div class="uploaded">{{ _('Uploaded') }}: <span data-bind="text: formatTimeAgo(date)"></span></div>
			<div class="size">{{ _('Size') }}: <span data-bind="text: formatSize(size)"></span></div>
			<div class="btn-group action-buttons">
				<a class="btn " data-bind="attr: {href: $root.downloadLink($data), css: {disabled: !$root.downloadLink($data)}}" title="{{ _('Download') }}"><i class="icon-download-alt"></i></a>
				<div class="btn " data-bind="click: function() { if ($root.enableRemove($data)) { $root.removeFile($data); } else { return; } }, css: {disabled: !$root.enableRemove($data)}" title="{{ _('Remove') }}"><i class="icon-trash"></i></div>
				<div class="btn "  data-bind="click: function() { if ($root.enableSlicing($data)) { $root.sliceFile($data); } else { return; } }, css: {disabled: !$root.enableSlicing($data)}" title="{{ _('Slice') }}"><i class="icon-magic"></i></div>
			</div>
		</script>

		<script type="text/html" id="bocusini_files_template_folder">
			<div class="title" data-bind="text: name"></div>
		</script>
	</div>
